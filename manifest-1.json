{
  "manifest_version": 3,
  "name": "E2e Messenger Loader",
  "version": "1.5",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon9.png"
  },
  "icons": {
    "128": "icon9.png"
  },
  "content_scripts": [{
    "matches": ["https://www.facebook.com/messages/*"],
    "js": ["content_script.js"]
  }]
}